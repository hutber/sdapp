<% if(typeof data !== "undefined"){ %>
<span class="spanicon">
	<i class="icon-down-open"></i>
	<select name="month" id="month">
		<% Object.keys(data).forEach(function(key){ var currentSex = data[key]; var dateSex = Date.parse(currentSex[0].sextime).toString("MMMM yyyy"); %>
			<option value="<%= key %>"><%= dateSex %> <span>(<%= currentSex.length %>)</span></option>
		<%	});  %>
	</select>
</span>
<div class="historyContent">

</div>

<% }else { %>
<div class="nostats">
	<h1>Seriously!? You still haven't recorded anything</h1>
	<p>Man... You need to get on and do something</p>
	<a href="#home">Go home... click self pleasure and record something</a>
</div>
<% } %>